  <SQLQuery>
    <![CDATA[select distinct sa.sup_id,gs.Sector_Code,gs.Sector_Name
from Sup_Alloc sa left outer join Sup_Shift_Sector_Alloc ss
on sa.Alloc_id=ss.Alloc_id
