  <SQLQuery>
    <![CDATA[SELECT SalesSettlementEO.Ack_status_id, 
       SalesSettlementEO.Sale_Stl_seq, 
       SalesSettlementEO.Alt_Chq_num, 
       SalesSettlementEO.Auth_Code, 
       SalesSettlementEO.CCD_Slip_Ack_Date, 
       SalesSettlementEO.CCD_Slip_Ack_User_ID, 
isnull(CASE (SalesSettlementEO.CCD_Type) 
  WHEN 'MC' THEN 'Master Card' 
  WHEN 'VI' THEN 'Visa' END,'NA')CCD_Type, 
       SalesSettlementEO.CCD_num, 
       SalesSettlementEO.Chq_Ack_Date, 
       SalesSettlementEO.Chq_Ack_User_ID, 
       SalesSettlementEO.Chq_Bank_name, 
       SalesSettlementEO.Chq_Date, 
       SalesSettlementEO.Chq_num, 
       SalesSettlementEO.PYMT_AMT, 
       SalesSettlementEO.PYMT_Type,  
       SalesSettlementEO.Sale_hdr_seq,
       hdr.Receipt_Num,
       hdr.Sale_Date,
       hdr.Location_Id,
       hdr.Esys_receipt_num,
       hdr.Sale_Date as Sale_Date_From,
       hdr.Sale_Date as Sale_Date_To
FROM  SLT_Sales_Settlement SalesSettlementEO,
slt_sales_header hdr
where SalesSettlementEO.Sale_hdr_seq = hdr.sale_hdr_seq]]>
  </SQLQuery>
