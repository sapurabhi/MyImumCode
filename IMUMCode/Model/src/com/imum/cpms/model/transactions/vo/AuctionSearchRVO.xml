PVT_SEARCH.Country_Code,
PVT_SEARCH.Foreign_Plate,
PVT_SEARCH.PLATE_NUMBER,
PVT_SEARCH.PLATE_CNT_ID,
PVT_SEARCH.PLATE_CTG_ID,
PVT_SEARCH.PLATE_TYPE_ID,
PVT_SEARCH.VEH_MAKE_ID,
PVT_SEARCH.VEH_MODEL_ID,
PVT_SEARCH.VEH_COLOUR_ID,
PVT_SEARCH.Active_Flag,
PVT_SEARCH.Blacklist_flag,
PVT_SEARCH.veh_ownership_type_id,
gim.CUST_NAME customer_name,
gim.Mobile_num Mobile_num,
PVT_SEARCH.ready_for_auction_flag
 FROM 
 (SELECT 
pvth.PVT_HDR_SEQ,
pvth.Request_log_seq,
pvth.PVT_Num,
pvth.Plate_Id,
pvth.PVT_SOURCE,
pvth.Contra_code,
pvth.Obsrv_seq,
pvth.Cust_ID,
pvth.Sector_Code,
pvth.Tariff_ID,
pvth.PI_ID,
pvth.HHC_ID,
pvth.PVT_date_time,
pvth.PVT_VHCL_geo_lat,
pvth.PVT_VHCL_geo_lon,
pvth.PVT_Status_ID,
pvth.PVT_STAGE_ID,
pvth.ENFORCE_STAGE_ID,
pvth.CLOSURE_REASONS,
pvth.CLOSURE_REMARKS,
pvth.CONTRA_AMT,
pvth.Discount_amt,
pvth.PVT_AMT_PAYABLE,
pvth.PVT_AMT_PAID,
pvth.verified_by,
pvth.verified_date,
pvth.DEF_City_Code,
pvth.DEF_Country_Mst,
pvth.ADP_number,
pvth.Appeal_number,
pvth.court_case_number,
pvth.zone_code,
pvth.Disc_Period_end_date,
pvth.Created_By,
pvth.Created_Date,
pvth.Updated_by,
pvth.Updated_date,
vpm.Country_Code,
vpm.Foreign_Plate,
vpm.PLATE_NUMBER,
vpm.PLATE_CNT_ID,
vpm.PLATE_CTG_ID,
vpm.PLATE_TYPE_ID,
pvth.VEH_MAKE_ID,
pvth.VEH_MODEL_ID,
pvth.VEH_COLOUR_ID,
Vpm.Active_Flag,
Vpm.Blacklist_Flag,
vpm.Cust_ID as Veh_CustId,
pvth.ready_for_auction_flag,
pvth.veh_ownership_type_id
from Pvt_Header pvth, Veh_Plate_Mst vpm
where pvth.Plate_Id=vpm.Plate_Id and pvth.ready_for_auction_flag='Y') PVT_SEARCH 
LEFT OUTER join 
(
-- select inst_id as cust_id,INST_NAME as CUST_NAME,''as Mobile_num  from     GEN_INSTITUTION_MST
 -- union
select cust_id,first_name as CUST_NAME,Mobile_num from  GEN_customer_MST)gim
On  Gim.Cust_Id = PVT_SEARCH.Veh_CustId]]>
