<?xml version="1.0" encoding="windows-1252"?>
<faces-config version="1.2" xmlns="http://java.sun.com/xml/ns/javaee">
  <application>
    <default-render-kit-id>oracle.adf.rich</default-render-kit-id>
    <locale-config>
      <default-locale>en</default-locale>
      <supported-locale>ar</supported-locale>
    </locale-config>
    <resource-bundle>
      <base-name>com.imum.cpms.view.ViewControllerBundle</base-name>
      <var>viewcontrollerBundle</var>
    </resource-bundle>
    <resource-bundle>
      <base-name>com.imum.cpms.model.ModelBundle</base-name>
      <var>modelBundle</var>
    </resource-bundle>
  </application>
  <navigation-rule>
    <from-view-id>/jspx/common/pages/Login.jspx</from-view-id>
    <navigation-case>
      <from-action>#{backingBeanScope.backing_LoginPage.onClickLogin_action}</from-action>
      <from-outcome>success.EN</from-outcome>
      <to-view-id>/jspx/common/pages/HomePageEnglish.jspx</to-view-id>
      <redirect/>
    </navigation-case>
    <navigation-case>
      <from-action>#{backingBeanScope.backing_LoginPage.onClickLogin_action}</from-action>
      <from-outcome>success.AR</from-outcome>
      <to-view-id>/jspx/common/pages/HomePageArabic.jspx</to-view-id>
      <redirect/>
    </navigation-case>

    <navigation-case>
      <from-action>#{backingBeanScope.backing_LoginPage.onClickResetPassword_action}</from-action>
      <from-outcome>ResetPassword</from-outcome>
      <to-view-id>/jspx/common/pages/ResetPassword.jspx</to-view-id>
      <redirect/>
    </navigation-case>
    
  </navigation-rule>
  <navigation-rule>
    <from-view-id>/jspx/common/pages/ResetPassword.jspx</from-view-id>
    <navigation-case>
      <from-action>#{backingBeanScope.backing_LoginPage.resetLogin_Link}</from-action>
      <from-outcome>resetLogin</from-outcome>
      <to-view-id>/jspx/common/pages/Login.jspx</to-view-id>
      <redirect/>
    </navigation-case>
  </navigation-rule>
  <navigation-rule>
    <from-view-id>/jspx/masters/fragments/PermitSearch.jsff</from-view-id>
    <navigation-case>
      <from-action>#{bindings.PermitSearchVO1.collectionModel.makeCurrent}</from-action>
      <from-outcome>success</from-outcome>
      <to-view-id>/jspx/masters/fragments/PermitDetails.jsff</to-view-id>
      <redirect/>
    </navigation-case>
  </navigation-rule>
  <navigation-rule>
    <from-view-id>/jspx/masters/fragments/PermitDetails.jsff</from-view-id>
    <navigation-case>
      <from-action>#{PermitSearchResMBean.cb1_action}</from-action>
      <from-outcome>Back</from-outcome>
      <to-view-id>/jspx/masters/fragments/PermitSearch.jsff</to-view-id>
      <redirect/>
    </navigation-case>
  </navigation-rule>
  <navigation-rule>
    <from-view-id>/jspx/transactions/fragments/BulkCards.jsff</from-view-id>
    <navigation-case>
      <from-action>#{BulkCardsMBean.cb1_action}</from-action>
      <from-outcome>success</from-outcome>
      <to-view-id>/jspx/masters/fragments/CreateCustomer.jsff</to-view-id>
    </navigation-case>
  </navigation-rule>
  <navigation-rule>
    <from-view-id>/jspx/transactions/fragments/BulkCards.jsff</from-view-id>
    <navigation-case>
      <from-action>#{BulkCardsMBean.createnewInstitution}</from-action>
      <from-outcome>Institution</from-outcome>
      <to-view-id>/jspx/masters/fragments/institution.jsff</to-view-id>
    </navigation-case>
  </navigation-rule>
  <navigation-rule>
    <from-view-id>/jspx/transactions/fragments/BulkCards.jsff</from-view-id>
    <navigation-case>
      <from-action>#{BulkCardsMBean.SubmitCards}</from-action>
      <from-outcome>Success</from-outcome>
      <to-view-id>/jspx/transactions/fragments/PaymentReceipt.jsff</to-view-id>
      <redirect/>
    </navigation-case>
  </navigation-rule>
  <lifecycle>
    <phase-listener id="nocache">com.imum.cpms.login.CacheControlPhaseListener</phase-listener>      
  </lifecycle>
  <navigation-rule>
    <navigation-case>
      <from-action>#{BulkCardsMBean.backBtnRcpt_action}</from-action>
      <from-outcome>backNav</from-outcome>
      <to-view-id>/jspx/transactions/fragments/BulkCards.jsff</to-view-id>
      <redirect/>
    </navigation-case>
  </navigation-rule>
</faces-config>